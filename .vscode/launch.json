{
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "KickerFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "KickerFunction"
      },
      "lambda": {
        "payload": {
          "json": {
            "queryStringParameters": {
              "angle": 50,
              "height": 7,
              "debug": "true"
            }
          }
        },
        "environmentVariables": {
          "ENV": "local"
        }
      }
    }
  ]
}
